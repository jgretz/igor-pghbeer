    "@nestjs/platform-express": "^8.2.4",
    "@nestjs/typeorm": "^8.0.2",
    "dotenv": "^11.0.0",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.2",
    "typeorm": "^0.2.41"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.1.8",
